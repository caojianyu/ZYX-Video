.advance{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 20rpx;
	margin: 0rpx 20rpx;
	-webkit-box-shadow: 0rpx 2rpx 10rpx 0rpx #F0F0F0;
	        box-shadow: 0rpx 2rpx 10rpx 0rpx #F0F0F0;
	-webkit-border-radius: 10rpx;
	        border-radius: 10rpx;
}
.advance view{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	text-align: center;
	width: 200rpx;
	height: 150rpx;
	color: white;
	font-size: 26rpx;
	padding: 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	-webkit-border-radius: 8%;
	        border-radius: 8%;
	-webkit-box-shadow: 0rpx 10rpx 10rpx 0rpx #F0F0F0;
	        box-shadow: 0rpx 10rpx 10rpx 0rpx #F0F0F0;
}
.advance view:nth-child(1){
	background-image: -webkit-linear-gradient(340deg,rgb(81, 142, 212),rgb(4, 89, 248));
	background-image: linear-gradient(110deg,rgb(81, 142, 212),rgb(4, 89, 248));
}
.advance view:nth-child(2){
	background-image: -webkit-gradient(linear,left top, left bottom,from(rgb(255, 72, 0)),to(rgb(230, 104, 46)));
	background-image: -webkit-linear-gradient(rgb(255, 72, 0),rgb(230, 104, 46));
	background-image: linear-gradient(rgb(255, 72, 0),rgb(230, 104, 46));
}
.advance view:nth-child(3){
	background-image: -webkit-gradient(linear,left top, left bottom,from(rgb(150, 4, 247)),to(rgb(144, 65, 235)));
	background-image: -webkit-linear-gradient(rgb(150, 4, 247),rgb(144, 65, 235));
	background-image: linear-gradient(rgb(150, 4, 247),rgb(144, 65, 235));
}
